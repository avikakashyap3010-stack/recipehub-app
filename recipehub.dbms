        // Current state
        let currentRecipes = [...recipes];
        let currentCategory = 'all';
        let currentRating = 0;
        let currentRecipeIndex = 0;
        let timerInterval = null;
        let timerSeconds = 0;
        let currentFeedbackRecipeId = null;

        // Initialize app when DOM loads
        document.addEventListener('DOMContentLoaded', function() {
            initApp();
        });

        function initApp() {
            // Login functionality
            document.getElementById('loginForm').addEventListener('submit', function(e) {
                e.preventDefault();
                const username = document.getElementById('username').value;
                const password = document.getElementById('password').value;
                
                if(username && password) {
                    document.getElementById('loginPage').style.display = 'none';
                    document.getElementById('recipeApp').style.display = 'block';
                    loadRecipes();
                }
            });

            // Logout functionality
            document.getElementById('logoutBtn').addEventListener('click', function() {
                document.getElementById('recipeApp').style.display = 'none';
                document.getElementById('loginPage').style.display = 'flex';
                document.getElementById('loginForm').reset();
            });

            // Navigation
            document.querySelectorAll('.nav-link').forEach(link => {
                link.addEventListener('click', function() {
                    const page = this.getAttribute('data-page');
                    showPage(page);
                    
                    // Update active nav
                    document.querySelectorAll('.nav-link').forEach(nav => nav.classList.remove('active'));
                    this.classList.add('active');
                });
            });

            // Category filtering
            document.querySelectorAll('.category-filter').forEach(filter => {
                filter.addEventListener('click', function() {
                    const category = this.getAttribute('data-category');
                    currentCategory = category;
                    filterRecipes(category);
                    
                    // Update active filter
                    document.querySelectorAll('.category-filter').forEach(f => f.classList.remove('active'));
                    this.classList.add('active');
                });
            });

            // Star rating
            document.querySelectorAll('.star').forEach(star => {
                star.addEventListener('click', function() {
                    currentRating = parseInt(this.getAttribute('data-rating'));
                    updateStars();
                    document.getElementById('ratingValue').value = currentRating;
                });
            });

            // Add recipe form
            document.getElementById('addRecipeForm').addEventListener('submit', function(e) {
                e.preventDefault();
                addNewRecipe();
            });

            // Feedback form
            document.getElementById('feedbackForm').addEventListener('submit', function(e) {
                e.preventDefault();
                submitFeedback();
            });

            // Search functionality
            document.getElementById('searchInput').addEventListener('keypress', function(e) {
                if(e.key === 'Enter') {
                    searchRecipes();
                }
            });

            // Recipe feedback modal
            document.querySelectorAll('.feedback-star').forEach(star => {
                star.addEventListener('click', function() {
                    const rating = parseInt(this.getAttribute('data-rating'));
                    updateFeedbackStars(rating);
                });
            });

            document.getElementById('submitRecipeFeedback').addEventListener('click', function() {
                submitRecipeFeedback();
            });

            document.getElementById('cancelFeedback').addEventListener('click', function() {
                document.getElementById('feedbackModal').classList.remove('active');
            });
        }

        // Load recipes into grid
        function loadRecipes() {
            const recipesGrid = document.getElementById('recipesGrid');
            const allRecipesGrid = document.getElementById('allRecipesGrid');
            
            displayRecipes(currentRecipes, recipesGrid);
            displayRecipes(recipes, allRecipesGrid);
        }

        // Display recipes in a grid
        function displayRecipes(recipesToShow, container) {
            container.innerHTML = '';
            
            if (recipesToShow.length === 0) {
                container.innerHTML = `
                    <div class="empty-state">
                        <h3>No Recipes Found</h3>
                        <p>Try a different search term or filter to find delicious recipes!</p>
                    </div>
                `;
                return;
            }
            
            recipesToShow.forEach((recipe, index) => {
                const recipeCard = `
                    <div class="recipe-card">
                        <div class="recipe-img" style="background-image: url('${recipe.image}')" onclick="showRecipeDetail(${index})"></div>
                        <div class="recipe-info">
                            <h3 onclick="showRecipeDetail(${index})">${recipe.name}</h3>
                            <p onclick="showRecipeDetail(${index})">${recipe.description}</p>
                            <div class="recipe-meta">
                                <span>‚è± ${recipe.time} mins</span>
                                <span>‚≠ê ${recipe.rating}</span>
                                <span>${getTypeIcon(recipe.type)}</span>
                            </div>
                            <div class="recipe-feedback">
                                <span>${getRandomReviews()} reviews</span>
                                <button class="feedback-btn" onclick="openFeedbackModal(${recipe.id})">
                                    <span>üí¨</span> Rate & Review
                                </button>
                            </div>
                        </div>
                    </div>
                `;
                container.innerHTML += recipeCard;
            });
        }

        // Show recipe detail page
        function showRecipeDetail(index) {
            currentRecipeIndex = index;
            const recipe = currentRecipes[index];
            
            const recipeDetailHTML = `
                <section class="hero">
                    <h1>${recipe.name}</h1>
                    <p>${recipe.description}</p>
                </section>

                <div class="recipe-detail">
                    <div class="recipe-header">
                        <div class="recipe-hero-img" style="background-image: url('${recipe.image}')"></div>
                        <div class="recipe-title">
                            <h1>${recipe.name}</h1>
                            <div class="recipe-meta-info">
                                <span>‚è± ${recipe.time} minutes</span>
                                <span>‚≠ê ${recipe.rating}/5</span>
                                <span>${getTypeIcon(recipe.type)} ${recipe.type.toUpperCase()}</span>
                            </div>
                        </div>
                    </div>
                    
                    <div class="recipe-content">
                        <div class="ingredients-section">
                            <h3>Ingredients</h3>
                            <ul class="ingredients-list">
                                ${recipe.ingredients.map(ingredient => `<li>${ingredient}</li>`).join('')}
                            </ul>
                        </div>
                        
                        <div class="instructions-section">
                            <h3>Instructions</h3>
                            <ol class="instructions-list">
                                ${recipe.instructions.map(instruction => `<li>${instruction}</li>`).join('')}
                            </ol>
                        </div>
                    </div>
                    
                    <div class="timer-section">
                        <h3>Cooking Timer</h3>
                        <div class="timer-display" id="timerDisplay">00:00</div>
                        <div class="timer-controls">
                            <button class="timer-btn start-btn" onclick="startTimer()">Start Timer</button>
                            <button class="timer-btn pause-btn" onclick="pauseTimer()">Pause</button>
                            <button class="timer-btn reset-btn" onclick="resetTimer()">Reset</button>
                        </div>
                    </div>
                    
                    <div class="recipe-nav">
                        <button class="nav-btn" onclick="showPrevRecipe()" ${currentRecipeIndex === 0 ? 'disabled' : ''}>
                            ‚Üê Previous Recipe
                        </button>
                        <button class="nav-btn" onclick="showHomePage()">
                            Back to Recipes
                        </button>
                        <button class="nav-btn" onclick="showNextRecipe()" ${currentRecipeIndex === currentRecipes.length - 1 ? 'disabled' : ''}>
                            Next Recipe ‚Üí
                        </button>
                    </div>
                </div>
            `;
            
            document.getElementById('recipe-detail').innerHTML = recipeDetailHTML;
            showPage('recipe-detail');
        }

        // Timer functions
        function startTimer() {
            if (timerInterval) return;
            
            timerInterval = setInterval(() => {
                timerSeconds++;
                updateTimerDisplay();
            }, 1000);
        }

        function pauseTimer() {
            clearInterval(timerInterval);
            timerInterval = null;
        }

        function resetTimer() {
            pauseTimer();
            timerSeconds = 0;
            updateTimerDisplay();
        }

        function updateTimerDisplay() {
            const minutes = Math.floor(timerSeconds / 60);
            const seconds = timerSeconds % 60;
            document.getElementById('timerDisplay').textContent = 
                `${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')}`;
        }

        // Navigation between recipes
        function showPrevRecipe() {
            if (currentRecipeIndex > 0) {
                showRecipeDetail(currentRecipeIndex - 1);
            }
        }

        function showNextRecipe() {
            if (currentRecipeIndex < currentRecipes.length - 1) {
                showRecipeDetail(currentRecipeIndex + 1);
            }
        }

        function showHomePage() {
            showPage('home');
            document.querySelectorAll('.nav-link').forEach(nav => nav.classList.remove('active'));
            document.querySelector('.nav-link[data-page="home"]').classList.add('active');
        }

        // Helper function for type icons
        function getTypeIcon(type) {
            const icons = {
                'veg': 'üå±',
                'nonveg': 'üçó',
                'vegan': 'üíö'
            };
            return icons[type] || '';
        }

        // Show specific page
        function showPage(pageName) {
            document.querySelectorAll('.page').forEach(page => {
                page.classList.remove('active');
            });
            document.getElementById(pageName).classList.add('active');
            
            // Reload recipes when switching to recipes page
            if (pageName === 'recipes' || pageName === 'home') {
                loadRecipes();
            }
        }

        // Filter recipes by category
        function filterRecipes(category) {
            if(category === 'all') {
                currentRecipes = [...recipes];
            } else {
                currentRecipes = recipes.filter(recipe => 
                    recipe.category === category || recipe.type === category
                );
            }
            
            // Update both grids
            const recipesGrid = document.getElementById('recipesGrid');
            const allRecipesGrid = document.getElementById('allRecipesGrid');
            
            displayRecipes(currentRecipes, recipesGrid);
            displayRecipes(currentRecipes, allRecipesGrid);
        }

        // Search recipes
        function searchRecipes() {
            const searchTerm = document.getElementById('searchInput').value.toLowerCase().trim();
            
            if(!searchTerm) {
                // If search is empty, show filtered recipes
                filterRecipes(currentCategory);
                return;
            }
            
            const filteredRecipes = recipes.filter(recipe => 
                recipe.name.toLowerCase().includes(searchTerm) || 
                recipe.description.toLowerCase().includes(searchTerm) ||
                recipe.ingredients.some(ing => ing.toLowerCase().includes(searchTerm))
            );
            
            currentRecipes = filteredRecipes;
            loadRecipes();
        }

        // Update star rating display
        function updateStars() {
            document.querySelectorAll('.star').forEach(star => {
                const starRating = parseInt(star.getAttribute('data-rating'));
                if(starRating <= currentRating) {
                    star.classList.add('active');
                } else {
                    star.classList.remove('active');
                }
            });
        }

        // Add new recipe
        function addNewRecipe() {
            const name = document.getElementById('recipeName').value;
            const category = document.getElementById('recipeCategory').value;
            const type = document.getElementById('recipeType').value;
            const description = document.getElementById('recipeDescription').value;
            const ingredients = document.getElementById('recipeIngredients').value.split('\n').filter(line => line.trim());
            const instructions = document.getElementById('recipeInstructions').value.split('\n').filter(line => line.trim());
            const time = parseInt(document.getElementById('cookingTime').value);
            const image = document.getElementById('recipeImage').value || "https://images.unsplash.com/photo-1556909114-f6e7ad7d3136?ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60";
            
            // Validation
            if (!name || !category || !type || !description || !ingredients.length || !instructions.length || !time) {
                alert('Please fill all required fields');
                return;
            }
            
            const newRecipe = {
                id: recipes.length + 1,
                name,
                category,
                type,
                description,
                ingredients,
                instructions,
                image,
                time,
                rating: 4.5
            };
            
            // Add to recipes array
            recipes.push(newRecipe);
            currentRecipes.push(newRecipe);
            
            // Reset form
            document.getElementById('addRecipeForm').reset();
            
            // Show success message
            document.getElementById('formMessage').innerHTML = 
                '<div class="message success">üéâ Recipe added successfully!</div>';
            
            // Reload recipes
            loadRecipes();
            
            // Clear message after 3 seconds
            setTimeout(() => {
                document.getElementById('formMessage').innerHTML = '';
            }, 3000);
        }

        // Submit feedback
        function submitFeedback() {
            const name = document.getElementById('feedbackName').value;
            const email = document.getElementById('feedbackEmail').value;
            const rating = document.getElementById('ratingValue').value;
            const message = document.getElementById('feedbackMessage').value;
            
            // Validation
            if (!name || !email || !rating || !message) {
                alert('Please fill all fields and select a rating');
                return;
            }
            
            // Reset form
            document.getElementById('feedbackForm').reset();
            currentRating = 0;
            updateStars();
            
            // Show success message
            document.getElementById('feedbackMessageDiv').innerHTML = 
                '<div class="message success">‚úÖ Thank you for your feedback!</div>';
            
            // Clear message after 3 seconds
            setTimeout(() => {
                document.getElementById('feedbackMessageDiv').innerHTML = '';
            }, 3000);
        }

        // Recipe feedback functions
        function openFeedbackModal(recipeId) {
            currentFeedbackRecipeId = recipeId;
            document.getElementById('feedbackModal').classList.add('active');
            document.getElementById('recipeFeedback').value = '';
            updateFeedbackStars(0);
        }

        function updateFeedbackStars(rating) {
            document.querySelectorAll('.feedback-star').forEach(star => {
                const starRating = parseInt(star.getAttribute('data-rating'));
                if(starRating <= rating) {
                    star.classList.add('active');
                } else {
                    star.classList.remove('active');
                }
            });
        }

        function submitRecipeFeedback() {
            const feedback = document.getElementById('recipeFeedback').value;
            const stars = document.querySelectorAll('.feedback-star.active').length;
            
            if (stars === 0) {
                alert('Please select a rating');
                return;
            }
            
            // In a real app, you would save this to a database
            // For now, we'll just show a success message
            document.getElementById('feedbackModal').classList.remove('active');
            
            // Show success message
            alert(`Thank you for your ${stars}-star rating! Your feedback has been recorded.`);
        }

        // Helper function to generate random review counts
        function getRandomReviews() {
            const counts = [12, 25, 38, 47, 56, 63, 72, 89, 104, 127];
            return counts[Math.floor(Math.random() * counts.length)];
        }
    </script>
</body>
</html>
